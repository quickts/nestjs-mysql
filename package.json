    "@types/node": "^12.7.2",
    "promise-mysql": "^4.1.0",
    "mocha": "^10.2.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.5.3"
  },
  "dependencies": {
