    "@types/node": "^12.0.0",
    "mocha": "^6.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "@types/mysql": "^2.15.6",
    "mysql": "^2.17.1"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quickts/nestjs-mysql.git"
